<%- include('../partials/header') %>
<header>Daily Log</header>
<table id="list">
  <thead>
    <tr>
      <th>Day</th>
      <th>Release<br>Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% days.forEach(function(d) { %>
      <tr>
        <td><%= d.title %></td>
        <td><%= d.releaseYear %></td>
        <td><a href="/days/<%= d._id %>">DAY DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>